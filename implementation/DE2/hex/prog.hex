// HEX	   BINARY				ASSEMBLY			REPRESENTED VALUE //
// 4h	   OpCode(3)|Dest. address(2)|Src. address/Immediate(8)		  //
////////////////////////////////////////////////////////////////////////
// Input x1 and y1
0400	// 001 00 00000000		LDS		%0	SW		x1
1800	// 110 00 00000000		WAIT0	%0
0500	// 001 01 00000000		LDS		%1	SW		y1
1900	// 110 01 00000000		WAIT0	%1
// Calculate parts of x2 and y2
0260	// 000 10 01100000		LDI		%2	0.75	a11
1200	// 100 10 00000000		MUL		%2	%0		a11*x1
0340	// 000 11 01000000		LDI		%3	0.5		a12
1301	// 100 11 00000001		MUL		%3	%1		a12*y1
0a03	// 010 10 00000011		ADD		%2	%3		(a11*x1)+(a12*y1)
14c0	// 101 00 11000000		MULI	%0	-0.5	x1*a21
1560	// 101 01 01100000		MULI	%1	0.75	y1*a22
0801	// 010 00 00000001		ADD		%0	%1		(x1*a21)+(y1*a22)
// Display x2 and y2
0e14	// 011 10 00010100		ADDI	%2	20		x2
1e00	// 111 10 00000000		WAIT1	%2
0cec	// 011 00 11101100		ADDI	%0	-20		y2
1800	// 110 00 00000000		WAIT0	%0

